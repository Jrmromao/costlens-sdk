{
  "name": "costlens",
  "version": "1.2.0",
  "description": "Smart AI cost optimization with quality-aware routing. Automatically routes expensive models to cheaper alternatives while maintaining quality.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:load": "jest tests/load.test.ts",
    "load-test": "ts-node scripts/load-test.ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "prepare": "husky install"
  },
  "keywords": [
    "ai",
    "cost-tracking",
    "cost-analytics",
    "openai",
    "anthropic",
    "claude",
    "api-monitoring",
    "cost-optimization",
    "analytics",
    "llm",
    "gpt",
    "monitoring",
    "developer-tools"
  ],
  "author": "CostLens.dev",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jrmromao/costlens-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/Jrmromao/costlens-sdk/issues"
  },
  "homepage": "https://costlens.dev",
  "peerDependencies": {
    "@anthropic-ai/sdk": ">=0.20.0",
    "openai": ">=4.0.0"
  },
  "devDependencies": {
    "@anthropic-ai/sdk": "^0.20.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.0.0",
    "jest": "^29.7.0",
    "husky": "^9.0.0",
    "lint-staged": "^15.0.0",
    "openai": "^4.0.0",
    "prettier": "^3.0.0",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": "prettier --write"
  }
}
